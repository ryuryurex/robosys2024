import sys
import random
import string

def genereter(length):
    chars = string.ascii_letters + string.digits
    password = ""
    for i in range(length):
        password += random.choice(chars)
    return password

for line in sys.stdin:
    line = line.strip()
    if line == "":
        print("入力がありません")
        continue
    try:
        num = int(line)
        if num < 1:
            print("1以上の正数を入力してください")
            continue
        password = genereter(num)
        print("生成したパスワード: " + password)
    except:
        print("無効な入力です")
