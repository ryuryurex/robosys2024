import sys
import random
import string

def generator(length):
    chars = string.ascii_letters + string.digits
    password = ""
    for i in range(length):
        password += random.choice(chars)
    return password

for line in sys.stdin:
    line = line.strip()
    if line == "":
        print("入力がありません")
        sys.exit(1)
    try:
        num = int(line)
        if num <= 0:
            print("1以上の正数を入力してください")
            sys.exit(1)
        password = generator(num)
        print("生成したパスワード: " + password)
    except ValueError:
        print("無効な入力です")
        sys.exit(1)
